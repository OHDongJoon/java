CREATE TABLE MONEY_TBL_02(
	CUSTNO NUMBER(6),
	SELENON NUMBER(8) PRIMARY KEY,
	PCOST NUMBER(8),
	AMOUNT NUMBER(4),
	PRICE NUMBER(8),
	PCODE VARCHAR2(4),
	SDATE DATE
);
