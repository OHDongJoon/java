DROP TABLE MEMBER_TBL_02 ;
CREATE TABLE MEMBER_TBL_02(
 CUSTNO NUMBER(6) PRIMARY KEY,
 CUSTNAME VARCHAR2(60),
 PHONE VARCHAR2(13),
 ADDRESS VARCHAR2(60),
 JOINDATE DATE,
 GRADE CHAR(1),
 CITY CHAR(2)
);
DROP SEQUENCE MEMBER_SEQ;
CREATE SEQUENCE MEMBER_SEQ START WITH 100001 MAXVALUE 999999 NOCACHE NOCYCLE;
